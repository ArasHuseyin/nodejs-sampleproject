<body>
    <div class="sidenav">
        {{>header}}
        <div class="title">
            <h2 class="section-header">Save Chat &nbsp;&nbsp;&nbsp;&nbsp;<a class="settings" href="/settings"><i
                        class="fas fa-cog"></i></a><br> Dashboard</h2> <br>
            <div class="container h-100 search-div">
                <div class="d-flex justify-content-left h-100">
                    <div class="searchbar">
                        <input class="search_input search-field" type="text" name="user-search" placeholder="Search...">
                        <a href="#" class="search_icon nn">{{>searchIcon}}</a>
                    </div>
                </div>
            </div>
            <br>
            <div class="list-group contact-list-alerts">

            </div>
        </div>
        <div class="messaging">
            <h3 class=" text-center">Recent Chats</h3>
        </div>

    </div>

    </div>
    </div>

    <div id="main" class="main">
        <h4 class="section-greeting">Welcome {{username}}</h4>
    </div>
    <div class="mesgs">
        <div class="msg_history">
            <!--

            <div class="outgoing_msg">
                <div class="sent_msg">
                    <p>Lorem ipsum dolor sit amet.</p>
                    <span class="time_date"> 11:01 AM | June 9</span>
                </div>
            </div>
            -->

        </div>
        <div class="type_msg"></div>
    </div>



</body>
